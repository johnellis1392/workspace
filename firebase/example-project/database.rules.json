{
    "rules": {
        "users": {
            "$userId": {
                ".read": "auth.uid === $userId",
                ".write": false,
                "checklists": {
                    ".write": "auth.uid === $userId",
                    ".indexOn": [
                        "title"
                    ]
                },
                "examples": {
                    ".write": "auth.uid === $userId",
                    ".indexOn": [
                        "title"
                    ]
                }
            }
        },
        "checklists": {
            "$checklistId": {
                ".read": false,
                ".write": false
            }
        },
        "examples": {
            "$exampleId": {
                ".read": false,
                ".write": false
            }
        }
    }
}
