{
    "rules": {
        "users": {
            "$userId": {
                "checklists": {
                    ".read": "auth.uid === $userId",
                    ".write": "auth.uid === $userId",
                    ".indexOn": [
                        "title"
                    ]
                },
                "examples": {
                    ".read": "auth !== null",
                    ".write": "auth.uid === $userId",
                    ".indexOn": [
                        "title"
                    ]
                }
            }
        }
    }
}
